{
  "name": "scoutapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "react-app-rewired build",
    "compile": "yarn update-config && tsc -p ./tsconfig.json --noEmit",
    "eject": "react-scripts eject",
    "test": "jest",
    "lint": "eslint './**/*.{ts,tsx}'",
    "update-config": "ts-node --project scripts/tsconfig.json scripts/update-config.ts",
    "update-localization": "node scripts/update-localization.js",
    "build:production": "env-cmd -f .env env-cmd -f .env.production react-app-rewired build",
    "build:staging": "env-cmd -f .env env-cmd -f .env.staging react-app-rewired build",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:push": "sh scripts/push-docker.sh",
    "graphql-generate": "graphql-codegen"
  },
  "dependencies": {
    "@types/node": "^13.13.5",
    "@types/react-native": "^0.62.7",
    "dotenv": "^8.2.0",
    "fs": "^0.0.1-security",
    "prettier": "^2.0.5",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "lint-staged": "^10.2.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ]
  }
}
